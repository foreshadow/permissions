{
  "name": "wbb/permissions",
  "description": "Powerful package for handling roles and permissions in Laravel 5.",
  "keywords": [
    "roles",
    "permissions",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "wbb(a.k.a. foreshadow @ github)"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "~5.5"
  },
  "autoload": {
    "psr-4": {
      "Wbb\\Permissions\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/database": "3.5.*",
    "orchestra/testbench": "3.5.*",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "satooshi/php-coveralls": "1.0.*",
    "sebastian/environment": "2.0.*"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/User.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
